@model IEnumerable<LTMSV2.Models.ImportShipment>


<thead>
    <tr>
        <th>
            Manifest Number
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MAWB)
        </th>
        <th>
            Manifest Date
        </th>
        <th>
            Flight Date
        </th>
        <th>
            Flight No
        </th>
        <th>
            Origin Country
        </th>
        <th>
            Origin Airport
        </th>
        <th>
            Destination Country
        </th>
        <th>
            Destination Airport
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ManifestNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MAWB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FlightDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FlightNo)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.ConsignorCountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OriginAirportCity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConsigneeCountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DestinationAirportCity)
        </td>

        <td>
                     
            
                <a href="EditImport?ImportId=@item.ID"><i class="fa fa-pencil"></i></a>
            

        </td>
    </tr>
    }
</tbody>
